<?php

/**
 * @file
 * Install and update functions for the Drupal Cloud Core profile.
 */

/**
 * Enable json_import module.
 */
function dcloud_core_update_9001() {
  // Enable json_import module as it's now a dependency for dcloud_import.
  \Drupal::service('module_installer')->install(['json_import']);
  
  return t('Enabled json_import module.');
}