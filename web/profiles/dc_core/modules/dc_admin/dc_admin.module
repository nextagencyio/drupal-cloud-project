<?php

/**
 * @file
 * Main module file for dc_admin.
 */

/**
 * Implements hook_page_attachments().
 */
function dc_admin_page_attachments(array &$attachments) {
  // Attach custom Gin theme styles to all admin pages
  if (\Drupal::service('router.admin_context')->isAdminRoute()) {
    $attachments['#attached']['library'][] = 'dc_admin/gin_custom';
  }
}
